{
    "name": "lucidtaz/yii2-scssphp",
    "description": "Yii2 bindings for leafo/scssphp",
    "type": "library",
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "^2.0",
        "leafo/scssphp": "^0.6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3"
    },
    "autoload": {
        "psr-4": { "lucidtaz\\yii2scssphp\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "lucidtaz\\yii2scssphp\\tests\\": "tests/" }
    },
    "authors": [
        {
            "name": "Thijs Zumbrink",
            "email": "thijs@schalpoen.nl"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
